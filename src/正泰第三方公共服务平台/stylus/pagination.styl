@import "./variable/color.styl"

.pagination
  font-size 12px
  color $textColor_1
  text-align center
  line-height 24px
  .pagination-current
    margin-right 14px
  .pagination-prov,
  .pagination-next
    text-decoration underline
  .pagination-begin,
  .pagination-end
    margin-left 9px
  .pagination-page_number
    display inline-block
    a
      margin 0 5px